{
  "name": "app",
  "version": "0.0.1",
  "license": "MIT",
  "config": {
    "coreui_library_short_version": "4.3",
    "coreui_library_docs_url": "https://coreui.io/angular/docs/"
  },
  "scripts": {
    "ng": "nx",
    "start": "nx serve",
    "build": "nx build",
    "watch": "nx build --watch --configuration development",
    "test": "nx test",
    "i18n:app:init": "ngx-translate-extract --input ./lib --output ./apps/app/src/assets/i18n/template.json --key-as-default-value --replace --format json",
    "i18n:app:extract-en": "ngx-translate-extract --sort --format namespaced-json --marker _i18n --format-indentation \"  \" --input ./libs --output ./apps/app/src/assets/i18n/en.json",
    "i18n:app:extract-es": "ngx-translate-extract --sort --format namespaced-json --marker _i18n --format-indentation \"  \" --input ./libs --output ./apps/app/src/assets/i18n/es.json",
    "i18n:app:extract-ar": "ngx-translate-extract --sort --format namespaced-json --marker _i18n --format-indentation \"  \" --input ./libs --output ./apps/app/src/assets/i18n/ar.json",
    "i18n:app:extract-all": "npm run i18n:app:extract-en && npm run i18n:app:extract-es && npm run i18n:app:extract-ar",
    "serve:app:prod": "npx nx serve app --c=prod",
    "serve:app:pre": "npx nx serve app --c=pre",
    "serve:app:dev": "npx nx serve app --c=dev",
    "check:app:fix": "eslint apps/app/src --ext .ts --fix",
    "check:app:prod": "eslint apps/app/src --ext .ts && ng build --c=prod",
    "build:app:prod": "npx nx build app --c=prod --aot --base-href ./",
    "build:app:pre": "npx nx build app --c=pre --aot --base-href ./",
    "build:app:dev": "npx nx build app --c=dev --aot --base-href ./",
    "deploy:app:copy": "cpx \"apps/app/src/package.json\" \"dist/apps/app/\"",
    "deploy:app:version": "cd \"dist/apps/app/\" && npm version",
    "deploy:app:publish": "cd \"dist/apps/app/\" && npm publish",
    "deploy:app:pack": "cd \"dist/apps/app/\" && npm pack"
  },
  "private": true,
